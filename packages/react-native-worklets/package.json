{
  "name": "react-native-worklets",
  "version": "0.0.1",
  "description": "Worklets for React Native",
  "scripts": {
    "build": "bob build",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ext '.js,.ts,.tsx' src && prettier --check src ",
    "format": "yarn format:js",
    "format:js": "prettier --write --list-different src",
    "type:check": "yarn type:check:src",
    "type:check:src": "tsc --noEmit"
  },
  "peerDependencies": {
    "@babel/core": "^7.0.0-0",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "prettier": "2.8.8",
    "react": "18.2.0",
    "react-native": "0.74.0",
    "react-native-builder-bob": "^0.18.3",
    "typescript": "~5.3.0"
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "module",
      "typescript"
    ]
  },
  "main": "lib/module/index",
  "module": "lib/module/index",
  "react-native": "src/index",
  "source": "src/index",
  "types": "lib/typescript/index",
  "packageManager": "yarn@4.1.1"
}
